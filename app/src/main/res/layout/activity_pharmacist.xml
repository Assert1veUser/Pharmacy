<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/tittle"
    android:padding="20dp"
    tools:context=".Pharmacist.PharmacistActivity">

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Фармацевт"
        android:textColor="#FFFFFF"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/tableLayout6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TableLayout
        android:id="@+id/tableLayout6"
        android:layout_width="match_parent"
        android:layout_height="74dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageButton
                android:id="@+id/butSearchMedicinesForCheck"
                android:layout_width="match_parent"
                android:layout_height="74dp"
                android:layout_weight="1"
                android:background="#FFFFFF"
                android:src="@drawable/baseline_search_24" />

            <ImageButton
                android:id="@+id/butFixDataInformation"
                android:layout_width="match_parent"
                android:layout_height="74dp"
                android:layout_weight="1"
                android:background="#FFFFFF"
                android:src="@drawable/baseline_restore_from_trash_24" />

            <ImageButton
                android:id="@+id/butUpdateStatus"
                android:layout_width="match_parent"
                android:layout_height="74dp"
                android:layout_weight="1"
                android:background="#FFFFFF"
                android:src="@drawable/baseline_add_24" />
        </TableRow>
    </TableLayout>

</androidx.constraintlayout.widget.ConstraintLayout>